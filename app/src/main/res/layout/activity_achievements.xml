            android:id="@+id/TextView03"
            android:text="Awards"
            android:paddingRight="20dp"
            android:textStyle="bold"
            android:typeface="serif"></TextView>


    </TableRow>
    <TableRow
        android:id="@+id/tableRow2"
        android:layout_height="wrap_content"
        android:layout_width="match_parent">
